{
  "name": "book-a-meal",
  "version": "1.0.0",
  "description": "Book-A-Meal is an application that allows customers to make food orders and helps the food vendor know what the customers want to eat.",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production nodemon --exec babel-node ./server/dist/app.js",
    "start:dev": "cross-env NODE_ENV=development nodemon --exec babel-node ./server/src/app.js",
    "build": "cross-env NODE_ENV=development babel ./server/src -d ./server/dist",
    "heroku-postbuild": "cross-env NODE_ENV=development babel ./server/src -d ./server/dist",
    "pretest": "",
    "lint": "eslint **/*.js --fix",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "test": "cross-env NODE_ENV=test nyc mocha ./server/tests/**/'*.test.js' --recursive --timeout 50000 --exit",
    "test:watch": "npm test --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emmygozi/Book-A-Meal.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/emmygozi/Book-A-Meal/issues"
  },
  "homepage": "https://github.com/emmygozi/Book-A-Meal#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "cross-env": "^5.1.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.2.1",
    "morgan": "^1.9.0",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.37.6",
    "validator": "^10.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-preset-env": "^1.6.1",
    "istanbul": "^0.4.5",
    "mocha": "^5.1.1",
    "nodemon": "^1.17.3",
    "nyc": "^11.7.1"
  }
}
